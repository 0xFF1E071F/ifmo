




       ;делимое	 A.B
      S equ R1     ;делитель 
	         	   ;B частное делимое
	  i equ R0

    cseg at 0
	  mov A,P0 ;делимое-старший байт
	  mov B,P1 ;делимое.частное
	  mov S,P2  ;делитель 
	  subb A,S	  ;	if(A>=S) {OV=1; goto out;}
	  jc sta		  ;пробное вычитание
	  setb ov
	  jmp out
 sta: 
      mov i,#10 
 m1:  add a,S  ;восстановление остатка
      clr c  
cikl: djnz i,m2 ;for (i=0 ; i<8; i++ )
      jmp out
m2:   
       xch a,B
      rlc A	   ;A= (((A<<1)-S)>=0) ? (A<<1)-S +1 : B<<1 ;	
      xch a,B	    
	  rlc a
	  mov F0,C
	  subb a,S		;первая цифра
	  jb F0,cikl
		jc m1
	   setb c
	   jmp cikl
out:
       mov P3,B  ; P3=B;частное	
	   sjmp $					
       end
  
